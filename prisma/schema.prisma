

generator client {
  provider = "prisma-client-js"
  output   = "../src/prisma"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model Ability {
  id              String   @id @default(uuid())
  ability_id      String   @unique @db.VarChar(191)
  ability_name    String
  description     String @db.LongText
  tactic          String
  technique_id    String
  technique_name  String
  platform        String
  shell_type      String
  command         String @db.MediumText
  payload         String @db.LongText
  type            String
  createdAt       DateTime @default(now())
  updatedAt       DateTime @updatedAt
}
 